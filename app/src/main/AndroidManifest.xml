<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fixit">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_fixit"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_fixit_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FixIt">
        <activity
            android:name=".Contato_Activity"
            android:exported="false" />
        <activity
            android:name=".Editar_Proposta_Activity"
            android:configChanges="orientation|screenSize"
            android:exported="false" />
        <activity
            android:name=".Nova_Proposta_Activity"
            android:exported="false" />
        <activity
            android:name=".Editar_Servico_Activity"
            android:exported="false" />
        <activity
            android:name=".Novo_Servico_Activity"
            android:exported="false" />
        <activity
            android:name=".Cliente_Activity"
            android:exported="false"
            android:label="@string/title_activity_cliente"
            android:parentActivityName=".Login_Activity"
            android:theme="@style/Theme.FixIt.NoActionBar" />
        <activity
            android:name=".Register_Activity_2"
            android:exported="true" />
        <activity
            android:name=".Register_Activity_1"
            android:exported="true" />
        <activity
            android:name=".Login_Activity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat"/> <!-- optional (needed if default theme has no action bar) -->
    </application>

</manifest>